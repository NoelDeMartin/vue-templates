{
  "name": "<%= shortName %>",
  "private": true,
  "scripts": {
    "serve": "serve",
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development webpack --mode='development' --progress --hide-modules",
    "watch": "cross-env NODE_ENV=development webpack --mode='development' --watch --progress --hide-modules",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --mode='development' --inline --hot",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production webpack --mode='production' --no-progress --hide-modules",
    "publish": "scripts/publish.sh"
  },
